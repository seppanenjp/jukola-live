{
  "name": "y",
  "version": "1.0.0",
  "description": "Jukola Live Stream",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "startdev": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "startdev:watch": "nodemon",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seppanenjp/jukola-live.git"
  },
  "keywords": [
    "Jukola",
    "Live",
    "Stream"
  ],
  "author": "Jukka-Pekka Sepp√§nen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seppanenjp/jukola-live/issues"
  },
  "homepage": "https://github.com/seppanenjp/jukola-live#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "http": "0.0.0",
    "prerender-node": "^3.2.5",
    "socket.io": "^2.3.0",
    "ts-node": "^8.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run startdev",
    "ext": "ts"
  },
  "engines": {
    "node": "12.16.x"
  }
}
